{
  "name": "architecture-draft",
  "version": "0.0.1",
  "description": "Containing a router and more",
  "main": "router/index.js",
  "scripts": {
    "prestart-plain": "babel . --extensions .es6 --out-dir .",
    "prestart-router": "babel . --extensions .es6 --out-dir .",
    "prestart-layout": "babel . --extensions .es6 --out-dir .",
    "prestart-catalog": "babel . --extensions .es6 --out-dir .",
    "prestart-general": "babel . --extensions .es6 --out-dir .",
    "prestart-legacy": "babel . --extensions .es6 --out-dir .",
    "prestart-cdn": "babel . --extensions .es6 --out-dir .",
    "pretest": "babel . --extensions .es6 --out-dir .",
    "compile": "babel . --extensions .es6 --out-dir .",
    "start-plain": "node --harmony router/index.js --port 3000 --config ../config/router.json --routes ./config/routes.json",
    "start-router": "node --harmony koa-router/index.js --port 3001 --config ../config/router.json --routes ../config/routes.json",
    "start-layout": "node --harmony layout/index.js --port 3002 --config ../config/layout.json --mode bigpipe",
    "start-catalog": "node --harmony fragments/catalog --port 3005  --config ../../config/catalog.json",
    "start-general": "node --harmony fragments/general --port 3003",
    "start-legacy": "node --harmony fragments/login --port 3004 --config ../../config/login.json",
    "start-cdn": "node --harmony cdn/index.js --port 3006",
    "prestart-pm2": "babel . --extensions .es6 --out-dir .",
    "start-pm2": "pm2 start pm2.json",
    "stop-pm2": "pm2 kill",
    "test": "mocha --harmony test/router --recursive"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "babel-cli": "^6.2.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.2.0",
    "babel-plugin-transform-es2015-parameters": "^6.1.18",
    "chai": "^3.4.1",
    "co": "^4.6.0",
    "commander": "^2.9.0",
    "cookies": "^0.5.1",
    "handlebars": "^4.0.5",
    "koa": "^1.1.2",
    "koa-body": "^1.3.0",
    "koa-bodyparser": "^2.0.1",
    "koa-compose": "^2.3.0",
    "koa-compress": "^1.0.8",
    "koa-convert": "^1.2.0",
    "koa-hbs": "^0.7.0",
    "koa-session": "^3.3.1",
    "koa-static": "^1.5.2",
    "lodash": "^3.10.1",
    "memcached": "^2.2.1",
    "mocha": "^2.3.4",
    "request": "^2.67.0",
    "sinon": "^1.17.2",
    "sinon-chai": "^2.8.0",
    "supertest": "^1.1.0",
    "uuid": "^2.0.1"
  }
}
