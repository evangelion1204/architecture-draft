{
  "name": "architecture-draft",
  "version": "0.0.1",
  "description": "Containing a router and more",
  "main": "router/index.js",
  "scripts": {
    "prestart-router": "babel . --extensions .es6 --out-dir .",
    "prestart-layout": "babel . --extensions .es6 --out-dir .",
    "prestart-catalog": "babel . --extensions .es6 --out-dir .",
    "prestart-general": "babel . --extensions .es6 --out-dir .",
    "prestart-legacy": "babel . --extensions .es6 --out-dir .",
    "pretest": "babel . --extensions .es6 --out-dir .",
    "start-router": "node --harmony router/index.js --port 3001 --config ../config/routes.json",
    "start-layout": "node --harmony layout/index.js --port 3002 --config ../config/layout.json",
    "start-catalog": "node --harmony fragments/all.js --port 3005",
    "start-general": "node --harmony fragments/all.js --port 3003",
    "start-legacy": "node --harmony fragments/all.js --port 3004 ",
    "test": "mocha --harmony --recursive test"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "babel-cli": "^6.2.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.2.0",
    "babel-plugin-transform-es2015-parameters": "^6.1.18",
    "chai": "^3.4.1",
    "commander": "^2.9.0",
    "koa": "^1.1.2",
    "koa-convert": "^1.2.0",
    "koa-hbs": "^0.7.0",
    "lodash": "^3.10.1",
    "mocha": "^2.3.4",
    "supertest": "^1.1.0"
  }
}
